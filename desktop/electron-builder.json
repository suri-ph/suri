{     
    "files": [
        "dist-electron/**/*",
        "dist-react/**/*"
    ],
    "extraResources": [
    {
      "from": "public/weights",
      "to": "weights",
      "filter": ["**/*"]
    }
  ],
    "extraFiles": [
        {
            "from": "src/electron/preload.js",
            "to": "dist-electron/electron/preload.js"
        }
    ],
    "appId": "com.suri.app",
    "productName": "Suri",
    "icon": "./picture.png",
    "win": {
        "target": ["portable", "msi"]
    },
    "linux": {
        "target": "AppImage",
        "category": "utility"
    },
    "mac": {
        "target": "dmg"
    }
}